   "source": [
    "# 2번 시간 섞기 VS 시간 유지\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "06e7c118",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
